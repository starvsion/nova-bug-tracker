{
    "name": "starvsion/nova-bug-tracker",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Starvsion\\NovaBugTracker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Starvsion\\NovaBugTracker\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
